<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.eDrink24.config.StoreMapper">

    <!-- 장바구니에 담긴 상품 불러오기 -->
    <select id="showAllStore" resultType="StoreDTO">
      select
        STORE.storeId,
        STORE.storeName,
        STORE.storeAddress,
        STORE.storePhoneNum
      from STORE
    </select>


























</mapper>